             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PTM.Terminal.TaskBoards"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <ResourceDictionary Source="/PTM.Terminal;component/Resources/MergedDictionary.xaml"/>
    </UserControl.Resources>
    <Grid>
        <Label HorizontalAlignment="Center" VerticalAlignment="Center" Content="{StaticResource IDS_TaskBoardListView_UserTaskBoards}" Foreground="{StaticResource BrushTextColor}" FontWeight="SemiBold" FontSize="25" FontFamily="Calibri"/>
    </Grid>
</UserControl>
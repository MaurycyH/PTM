<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="{x:Type Button}">
        <Setter Property="Margin" Value="5"/>
        <Setter Property="Width" Value="50"/>
        <Setter Property="Height" Value="30"/>
    </Style>
    <!-- Colors -->
    <Color x:Key="TextPanelColor">#EAEAED</Color>
    <Color x:Key="TextColor">#161A3B</Color>
    <Color x:Key="TextPanelIsMouseOverColor">#212529</Color>
    <Color x:Key="BackgroundMainBarColor">#464775</Color>
    <Color x:Key="BackgroundSideBarColor">#33344A</Color>
    <!-- Brushes -->
    <SolidColorBrush x:Key="BrushTextPanelColor" Color="{StaticResource TextPanelColor}"/>
    <SolidColorBrush x:Key="BrushTextColor" Color="{StaticResource TextColor}"/>
    <SolidColorBrush x:Key="BrushTextPanelIsMouseOverColor" Color="{StaticResource TextPanelIsMouseOverColor}"/>
    <SolidColorBrush x:Key="BrushBackgroundMainBarColor" Color="{StaticResource BackgroundMainBarColor}"/>
    <SolidColorBrush x:Key="BrushBackgroundMainBarColorClicked" Color="{StaticResource BackgroundSideBarColor}"/>
</ResourceDictionary>